{
  "name": "APP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android": "cd android && ./gradlew clean && cd ../ && react-native run-android",
    "ios": "cd ios && xcodebuild clean && cd ../ && react-native run-ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "aab": "npx jetify && cd android && ./gradlew clean build && ./gradlew bundleRelease && cd ../",
    "apk": "npx jetify && cd android && ./gradlew clean build && ./gradlew assembleRelease && cd ../",
    "postinstall": "npx typesync && npx pod-install ios",
    "genimg": "node genimg src/assets/images --output src/res/images.ts --name images",
    "build:android": "cd ./android && fastlane uploadAPK && cd ../",
    "build:ios": "cd ./ios && fastlane uploadIPA && cd ../",
    "genfont": "node genimg src/assets/fonts --output src/res/fonts.ts --name fonts",
    "push:android": "appcenter codepush release-react -a gnurt250394/APP-ORDER-SEOULLIFE-ANDROID -d Production",
    "push:ios": "appcenter codepush release-react -a gnurt250394/APP-ORDER-SEOULLIFE-IOS -d Production",
    "pushApp": "yarn push:android && yarn push:ios"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
